<header>
  テスト
</header>

<div class="back">
  <%= link_to root_path, class: 'nav-link' do %>
    <div><i class="fas fa-arrow-left"></i>戻る</div>
  <% end %>
</div>

<div class="level-container">
  <% @level.each do |level| %>
    <div class="level-wrapper">
      <div class="level-name">
        <%= level.name %>
      </div>
      <div class="radius-btn-wrapper">
        <%= link_to "スタート", question_level_path(level), class: "nav-link btn radius-btn btn-blue" %>
      </div>
    </div>
  <% end %>
</div>