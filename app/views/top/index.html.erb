<% unless user_signed_in? %>
  <div>
    <%= link_to "ログイン", new_user_session_path, class: "nav-link login-btn" %>
  </div>
  <div>
    <%= link_to "新規登録", new_user_registration_path, class: "nav-link login-btn" %>
  </div>
<% else %>
  <%= current_user.username %>
  <%= current_user.email %>
  <%= link_to "ログアウト", destroy_user_session_path, method: "delete", class: "nav-link" %>
<% end %>
